#include <iostream>
using namespace std;
int main (){
int t; cin>>t;
int n,k;
for(int i=0;i<t;i++){
    cin>>n>>k;
    int x;
    int dem = 0;
    for(int i = 0;i<n;i++){
        cin>>x;
        if(x>0){
            dem++;
        }
    }if(dem<k) cout<<"NO"<<endl;
    else cout<<"YES"<<endl;

}
}

